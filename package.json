    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
