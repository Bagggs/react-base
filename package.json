  "jest": {
    "roots": [
      "app/components",
      "app/lib"
    ],
    "modulePaths": [
      "app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^(.*)(s|S)tyles$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./app/lib/testMocks.js"
    ]
  }
