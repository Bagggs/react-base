{
  "name": "react-base",
  "version": "1.1.0",
  "authors": [
    "Marat Fakhreev <marat.fakhreev@flatstack.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:fs/react-base.git"
  },
  "scripts": {
    "stylelint": "stylelint ./app/{**,**/**}/*.css",
    "stylelint-fix": "stylefmt --recursive './app/{**,**/**}/*.css'",
    "eslint": "eslint server.js ./app ./config --ext .js,.jsx",
    "eslint-fix": "eslint server.js ./app ./config --ext .js,.jsx --fix",
    "lint": "npm run stylelint && npm run eslint",
    "jest": "jest",
    "test": "npm run lint",
    "start": "node ./server"
  },
  "license": "MIT",
  "engines": {
    "node": "7.8.x"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
