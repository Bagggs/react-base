    "qs": "^6.4.0",
    "react": "^15.5.3",
    "react-addons-css-transition-group": "^15.5.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.5.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
