    "postcss-inline-comment": "^3.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-pxtorem": "^4.0.0",
    "postcss-simple-vars": "^3.1.0",
    "react-addons-test-utils": "^15.5.1",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
